<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.chart.ScatterChart?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
          minWidth="-Infinity" prefHeight="714.0" prefWidth="820.0" xmlns="http://javafx.com/javafx/8.0.111"
          fx:controller="WTReview.Controller">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="20.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="20.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="60.0" prefWidth="100.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="60.0"/>
        <RowConstraints vgrow="ALWAYS"/>
    </rowConstraints>
    <children>
        <TilePane alignment="BOTTOM_LEFT" hgap="10.0" maxHeight="47.0">
            <children>
                <Button mnemonicParsing="false" onAction="#handleMAddButtonAction" text="+"/>
                <Button mnemonicParsing="false" text="-"/>
                <Button mnemonicParsing="false" text="^"/>
                <Button mnemonicParsing="false" text="v"/>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </TilePane>
        <TilePane alignment="BOTTOM_LEFT" hgap="10.0" maxHeight="47.0" GridPane.columnIndex="1">
            <children>
                <Button mnemonicParsing="false" onAction="#handleRAddButtonAction" text="+"/>
                <Button mnemonicParsing="false" text="-"/>
                <Button mnemonicParsing="false" text="^"/>
                <Button mnemonicParsing="false" text="v"/>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </TilePane>
        <ListView fx:id="ui_mList" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                  prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="1"/>
        <ListView fx:id="ui_rList" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                  prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
        <ScatterChart fx:id="ui_Graph" GridPane.columnIndex="2" GridPane.rowIndex="1">
            <xAxis>
                <NumberAxis side="BOTTOM"/>
            </xAxis>
            <yAxis>
                <NumberAxis side="LEFT"/>
            </yAxis>
        </ScatterChart>
    </children>
</GridPane>

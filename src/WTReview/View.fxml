<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
          minWidth="-Infinity" prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8.0.111"
          fx:controller="WTReview.Controller">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="60.0"/>
        <RowConstraints vgrow="ALWAYS"/>
    </rowConstraints>
    <children>
        <SplitPane dividerPositions="0.2982885085574572" prefHeight="160.0" prefWidth="200.0" GridPane.rowIndex="1">
            <items>
                <GridPane prefHeight="652.0" prefWidth="239.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints vgrow="ALWAYS"/>
                        <RowConstraints vgrow="NEVER"/>
                    </rowConstraints>
                    <children>
                        <ListView fx:id="ui_mList" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                                  prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1"/>
                        <TilePane alignment="CENTER_LEFT" hgap="10.0" maxHeight="47.0" prefRows="1"
                                  GridPane.columnIndex="1" GridPane.rowIndex="1">
                            <children>
                                <Button mnemonicParsing="false" onAction="#handleAddMeasuredButtonAction" text="+"/>
                                <Button mnemonicParsing="false" onAction="#handleMeasuredDelete" text="-"/>
                                <Button mnemonicParsing="false" onAction="#handleMeasuredMoveUp" text="^"/>
                                <Button mnemonicParsing="false" onAction="#handleMeasuredMoveDown" text="v"/>
                            </children>
                            <padding>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                            </padding>
                        </TilePane>
                        <ListView fx:id="ui_rList" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                                  prefHeight="200.0" prefWidth="200.0"/>
                        <TilePane alignment="CENTER_LEFT" hgap="10.0" maxHeight="47.0" prefRows="1"
                                  GridPane.rowIndex="1">
                            <children>
                                <Button mnemonicParsing="false" onAction="#handleAddReferenceButtonAction" text="+"/>
                                <Button mnemonicParsing="false" onAction="#handleReferenceDelete" text="-"/>
                                <Button mnemonicParsing="false" onAction="#handleReferenceMoveUp" text="^"/>
                                <Button mnemonicParsing="false" onAction="#handleReferenceMoveDown" text="v"/>
                            </children>
                            <padding>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                            </padding>
                        </TilePane>
                    </children>
                </GridPane>
                <VBox fx:id="chartPane" alignment="TOP_CENTER">
                    <children>
                        <TextArea fx:id="ui_ResultsTable" editable="false"/>
                        <LineChart fx:id="ui_ProfileGraph" legendSide="RIGHT" prefHeight="10000.0">
                            <xAxis>
                                <NumberAxis label="Position (mm)" lowerBound="-100.0" side="BOTTOM"/>
                            </xAxis>
                            <yAxis>
                                <NumberAxis label="Relative Height / Gamma Value" side="LEFT" upperBound="1.0"/>
                            </yAxis>
                        </LineChart>
                        <LineChart fx:id="ui_AnalysisGraph" legendSide="RIGHT" prefHeight="10000.0">
                            <xAxis>
                                <NumberAxis label="Depth (mm)" side="BOTTOM" upperBound="200.0"/>
                            </xAxis>
                            <yAxis>
                                <NumberAxis autoRanging="false" forceZeroInRange="false"
                                            label="Ratio (Measured / Reference)" lowerBound="0.95" side="LEFT"
                                            tickUnit="0.01" upperBound="1.05"/>
                            </yAxis>
                        </LineChart>
                    </children>
                </VBox>
            </items>
        </SplitPane>
        <TilePane alignment="CENTER_RIGHT" hgap="10.0" prefHeight="200.0" prefWidth="200.0">
            <children>
                <RadioButton fx:id="ui_SyncLists" mnemonicParsing="false" text="Sync?"/>
                <TextField fx:id="ui_coordinates" prefHeight="27.0" prefWidth="383.0"/>
                <Button fx:id="resetBtn" mnemonicParsing="false" text="Reset Zoom"/>
            </children>
        </TilePane>
    </children>
</GridPane>
